<?xml version="1.0" encoding="utf-8"?>

    <Shell
        x:Class="ShellTest.AppShell"
        xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
        xmlns:local="clr-namespace:ShellTest">

        <ShellContent FlyoutItemIsVisible="False"
                      ContentTemplate="{DataTemplate local:FirstPage}"/>
        <ShellContent FlyoutItemIsVisible="False"
                      ContentTemplate="{DataTemplate local:SecondPage}"/>
        <ShellContent FlyoutItemIsVisible="False"
                       ContentTemplate="{DataTemplate local:ThirdPage}"/>
    
        <FlyoutItem Route="Home" Title="One">
            <ShellContent
                ContentTemplate="{DataTemplate local:MainPage}"/>
        </FlyoutItem>
    </Shell>